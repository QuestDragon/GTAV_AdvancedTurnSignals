<?xml version="1.0" encoding="utf-8"?>
<!--DefaultSettingsタグは削除しないでください！正常に機能しなくなってしまいます。
タグ内の値（音声ファイル名）のみ変更するようにしてください。
Do not remove the DefaultSettings tag! It will no longer function properly.
Please only change the value within the tag (audio file name).-->

<TurnSignalSoundSetup>
  <DefaultSettings>
    <TURN_LEVER_INTRO>TURN_LEVER_INTRO</TURN_LEVER_INTRO>
    <TURN_LEVER_OUTRO>TURN_LEVER_OUTRO</TURN_LEVER_OUTRO>
    <INDICATOR_SOUND>INDICATOR_SOUND</INDICATOR_SOUND>
    <HAZARD_BUTTON>HAZARD_BUTTON</HAZARD_BUTTON>
  </DefaultSettings>

  <SoundSetup>
    <Vehicles>
      <Vehicle>SAMPLE</Vehicle>
    </Vehicles>
    <Sounds>
      <TURN_LEVER_INTRO>TURN_LEVER_INTRO_SAMPLE</TURN_LEVER_INTRO>
      <TURN_LEVER_OUTRO>TURN_LEVER_OUTRO_SAMPLE</TURN_LEVER_OUTRO>
      <INDICATOR_SOUND>INDICATOR_SOUND_SAMPLE</INDICATOR_SOUND>
      <HAZARD_BUTTON>HAZARD_BUTTON_SAMPLE</HAZARD_BUTTON>
    </Sounds>
  </SoundSetup>
  
</TurnSignalSoundSetup>